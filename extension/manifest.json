{
  "manifest_version": 2,
  "name": "SABKids",
  "description": "Search And Block",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "labelAndWrite.js"
      ],
      "html": [
        "popup.html"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}